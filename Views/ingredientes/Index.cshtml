@model IEnumerable<MVC.Models.ingredientes>

@{
    ViewBag.Title = "Index";
}

<h2>Control de Ingredientes</h2>

<p style="width:15%; padding:10px; border-radius:15px; background-color:springgreen;text-align:center; text-decoration-color:white;">
    @Html.ActionLink("Nuevo Ingrediente", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.tipoIngrediente)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nomIngrediente)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cantidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.inventario)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.tipoIngrediente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nomIngrediente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.cantidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.inventario)
            </td>
            <td style="border-radius:20px; background-color:springgreen; text-align:center">
                @Html.ActionLink("Editar", "Edit", new { id = item.idIngrediente })
                               
            </td>
            <td style="border-radius:20px; background-color:blue; text-align:center">
                @Html.ActionLink("Detalles", "Details", new { id = item.idIngrediente }) 
            </td>
            <td style="border-radius:20px; background-color:red; text-align:center">
                @Html.ActionLink("Eliminar", "Delete", new { id = item.idIngrediente })
            </td>
        </tr>
    }

</table>
<div style="  margin:4%; float:left; height: 200px; width: 25%; background-image: url(data:image/jpeg;base64,)">

</div>
<div style="margin: 4%; float: left; height: 200px; width: 25%; background-image: url(https://www.objetivobienestar.com/uploads/s1/36/60/12/Recetas-ingredientes-basicos-portada_1_780x462.jpg)"></div>
<div style="margin: 4%; float: left; height: 200px; width: 25%; background-image: url(https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQ5DZBtYFVp3y1ps78EHJ46gIbnGSRTxiVbEg&usqp=CAU) "></div>
